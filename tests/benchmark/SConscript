Import('env skia_lib qcheck_lib qcheck_imp')

_srcs = Glob('*.d')

_d_link_flags= env['LINKFLAGS'] + ['-lphobos2', '-lpthread', '-lm']

app = env.Program('benchmark',
                  _srcs,
#                  LINKCOM=env['DLINKCOM'],
                  LINKFLAGS=_d_link_flags,
                  LIBS=[skia_lib, qcheck_lib, 'freetype', 'z'],
                  DPATH=['#/src', '.', qcheck_imp])
Return('app')
