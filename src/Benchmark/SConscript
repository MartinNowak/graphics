Import('env skia_lib')

_srcs = Glob('*.d')

_d_link_flags=[File('ConsoleApp.def')]

app = env.Program('Benchmark',
                  _srcs,
                  LINKCOM=env['DLINKCOM'],
                  DLINKFLAGS=_d_link_flags,
                  LIBS=[skia_lib],
                  DPATH=['#/src', '.'])
Return('app')
