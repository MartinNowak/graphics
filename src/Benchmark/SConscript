Import('env skia_lib')

_srcs = Glob('*.d')

_d_link_flags=['-m32', '-lphobos2', '-lpthread', '-lm']

app = env.Program('Benchmark',
                  _srcs,
#                  LINKCOM=env['DLINKCOM'],
                  LINKFLAGS=_d_link_flags,
                  LIBS=[skia_lib],
                  DPATH=['#/src', '.'])
Return('app')
